{
  "name": "product-page-example-11ty",
  "version": "1.0.0",
  "description": "An 11ty based static site generator for GDS Portfolio product pages",
  "main": "index.js",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "npm run css:build && npx eleventy",
    "css:dev": "node-sass --watch scss/ -o css/ --include-path node_modules/govuk-frontend/govuk",
    "css:build": "node-sass scss/ -o css/ --include-path node_modules/govuk-frontend/govuk --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/product-page-example-11ty.git"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "node",
    "static",
    "nunjucks",
    "govuk",
    "design",
    "system"
  ],
  "author": "Jon Heslop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphagov/product-page-example-11ty/issues"
  },
  "homepage": "https://github.com/alphagov/product-page-example-11ty#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0-beta.2",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "govuk-frontend": "^3.0.0",
    "luxon": "^1.17.2",
    "markdown-it": "^9.1.0",
    "markdown-it-anchor": "^5.2.4",
    "node-sass": "^4.12.0"
  }
}
